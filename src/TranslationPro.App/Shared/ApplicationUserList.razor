
@if (Application != null)
{
    <table class="table">
        <thead>
        <tr>
            <th>Name</th>
            <th>Status</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var user in Application.Users)
        {
            <tr>
                <td>@user.Name</td>
                <td>@user.Status</td>
               
            </tr>
        }
        </tbody>
    </table>
}
